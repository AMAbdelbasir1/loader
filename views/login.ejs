<!DOCTYPE html>
<html>
  <head>
    <title>Login Page</title>
    <link rel="stylesheet" href="../css/login.css" />
  </head>
  <body>
    <div class="container">
      <div class="login-box">
        <h2>Login</h2>

        <div class="social-login">
          <div class="btn" id="googleBtn">
            <img src="/images/google_logo.png" alt="Google Logo" />
            Login with Google
          </div>

          <div class="btn" id="facebookBtn">
            <img src="/images/facebook_logo.png" alt="Facebook Logo" />
            Login with Facebook
          </div>
        </div>
      </div>
    </div>

    <script src="../js/en-login.js"></script>
  </body>
</html>
